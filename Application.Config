
[Config]
;按键映射相关配置文件的目录
Directory=.Config
;Directory=.Test
;长映射模式的检测延时
LongMappingDetectionDelay=-1

;路径字符替换
[PathReplace]
\@= ahk_exe 
@= ahk_exe 

#.Global=.*
#.Application=

#AutoHotkey=i).Ahk@Code.exe
#WebBrowser=ahk_exe i)(Chrome|msedge.exe)

;文件名字符替换
[NameReplace]

;单个修饰键的映射
[Modifier]
Capslock={Blind}{Esc}
Numlock={Numlock}
Appskey={Appskey}
XButton1={XButton1}
XButton2={XButton2}

[Control]
Modifier=Capslock
Reload=Esc
AdminRun=+Esc
Suspend=^+Space
ExitApp=+^End
;ListHotkeys=F12
KeyHistory=F12
;ListVars=F12
;ResetToggle 清除所有自定义切换态

[Setting]
KeyDelay=-1,-1
;KeyDelay=100,10
SendMode=Input
;#InputLevel指令 默认的输入等级, 热键和热字串的SendLevel默认等于InputLevel
InputLevel=50
;SendLevel, 设置输出等级, 用于激活其他脚本的热键/热字串
SendLevel=50